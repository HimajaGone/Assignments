package com.patterns.practice;

public class Pattern1 {

	public static void main(String[] args) {
		int i,j,k;
		for( i=1;i<=9;i++) {
			for( j=9;j>=i;j--) {
				System.out.print("");
			}
			System.out.print("*");
			for(k=1;k<2*(i-1);k++) {
				System.out.print("");
			}
			if(i==1)
					System.out.println("");
			else
				System.out.println("*");
		}
			//	for(int k=5;k<=9)
			}
//			for(k=5;k<=9;k++) {
//				System.out.print("* ");
//			}
//			System.out.println();
//			if(j==1) {
//				for(j=2;j<=5;j++) {
//					
//				}
//			}
		//}

	}

}
