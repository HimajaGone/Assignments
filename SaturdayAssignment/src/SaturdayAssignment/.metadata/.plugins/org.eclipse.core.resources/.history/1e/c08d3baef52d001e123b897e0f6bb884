package com.patterns.practice;

public class Pattern1 {

	public static void main(String[] args) {
		int i,j;
		for( i=1;i<=9;i++) {
			for( j=5;j>=1;j--) {
				System.out.print("* ");
				
			//	for(int k=5;k<=9)
			}
			System.out.println();
			if(j==1) {
				for(j=2;j<=5;j++) {
					
				}
			}
		}

	}

}
